<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alicee_pc.machine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Home"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MachineDisplay"
            android:label="@string/title_activity_machine_display"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddMachine"
            android:label="@string/title_activity_add_machine"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BinDetail"
            android:label="@string/title_activity_bin_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ListMyMachine" />
        <activity android:name=".ShowBin" />
        <activity android:name=".Settings" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>